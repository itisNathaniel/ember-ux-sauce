<div class="uxs">

  {{#uxs-app fixedNav=true}}

    {{#uxs-navbar fixed=true style="dark" name="App Demo" as |bar|}}
      {{bar.logo "sauce-logo.desktop" class="sm:hidden sm:invisible"}}
      {{bar.logo "sauce-logo.mobile" class="lg:hidden lg:invisible"}}
      {{#bar.desktopNav as |nav|}}
        {{#nav.menu detail=true align="right" as |menu|}}
          {{menu.link "Docs" "docs" rightIcon="book"}}
          {{menu.link "Api" "docs"}}
          {{menu.link "Dashboard" "app.dash"}}
          {{menu.link "People" "app.list"}}
          {{!-- {{menu.link "Single" "app.single"}}
          {{menu.link "Double" "app.double"}}
          {{menu.link "Triple" "app.triple"}} --}}
        {{/nav.menu}}
      {{/bar.desktopNav}}
      {{#bar.mobileNav as |nav|}}
        {{uxs-subheading "AppName" align="center"}}
        {{#nav.menu as |menu|}}
          {{menu.link "Docs" "docs" rightIcon="book"}}
          {{menu.link "Api" "docs"}}
          {{menu.link "Dashboard" "app.dash"}}
          {{menu.link "People" "app.list"}}
          {{!-- {{menu.link "Single" "app.single"}}
          {{menu.link "Double" "app.double"}}
          {{menu.link "Triple" "app.triple"}} --}}
        {{/nav.menu}}
      {{/bar.mobileNav}}
    {{/uxs-navbar}}

    {{outlet}}

  {{/uxs-app}}

</div>
