{{#uxs-data
  display="list"
  padded=false
  inset=false
as |data|
}}
  {{data.item
    label="Name"
    value=(concat model.first_name " " model.last_name)
  }}
  {{v.item
    label="Email"
    value=model.email
  }}
  {{data.item
    label="Gender"
    value=model.gender
  }}
  {{data.item
    label="IP Address"
    value=model.ip_address
  }}
{{/uxs-data}}
