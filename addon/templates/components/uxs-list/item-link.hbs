{{yield
  (
    hash
    parent=this
    body=(
      component
      "uxs-div"
      base="uxs-list__body"
      m=m
    )
    aside=(
      component
      "uxs-div"
      base="uxs-list__aside"
      m=m
    )
    title=(
      component
      "uxs-text"
      base="uxs-list__title"
      m=m
    )
    meta=(
      component
      "uxs-text"
      base="uxs-list__meta"
      m=m
    )
    avatar=(
      component
      "uxs-list/avatar"
      m=m
    )
    action=(
      component
      "uxs-icon"
      base=(concat b "__action")
    )
  )
}}
