{{#uxs-menu style="drawer" as |menu|}}
  {{#if loading}}
    {{#each items as |item|}}
      {{menu.item item.params.firstObject disabled=true}}
    {{/each}}
  {{else}}
    {{#each items as |item|}}
      {{menu.link
        params=item.params
        disabled=item.disabled
        currentWhen=item.currentWhen
      }}
    {{/each}}
  {{/if}}
{{/uxs-menu}}
