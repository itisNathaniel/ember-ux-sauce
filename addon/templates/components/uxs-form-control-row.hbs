{{yield
  (
    hash

    control=(
      component
      "uxs-form-control"
      model=model
      disabled=disabled
      name=name
      validateOnFocus=validateOnFocus
      style=style
      rowControl=true
    )
    label=(
      component
      "uxs-form-label"
      name=name
      disabled=disabled
      inline=inline
      style=style
    )
    input=(
      component
      "uxs-form-input"
      autoresize=autoresize
      model=model
      name=name
      disabled=disabled
      inline=inline
      style=style
    )
    textarea=(
      component
      "uxs-form-textarea"
      autoresize=autoresize
      model=model
      name=name
      disabled=disabled
      inline=inline
      style=style
    )
    dropdown=(component "uxs-form-dropdown" model=model name=name disabled=disabled inline=inlinev)
    date=(component     "uxs-form-date"     model=model name=name disabled=disabled inlineInput=inline style=style)
    datetime=(component "uxs-form-datetime" model=model name=name disabled=disabled inlineInput=inline style=style)
    time=(component     "uxs-form-time"     model=model name=name disabled=disabled inlineInput=inline style=style)
    button=(component   "uxs-button"        name=name disabled=disabled inline=inline style=style)
    link=(component     "uxs-button-link"   name=name disabled=disabled inline=inline style=style)
    icon=(component     "uxs-icon"          name=name disabled=disabled inline=inline base="uxs-form__icon" style=style)
  )
}}
