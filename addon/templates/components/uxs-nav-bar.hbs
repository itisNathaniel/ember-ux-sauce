{{yield (hash
  logo=(component "uxs-nav-bar/logo")
  title=(component "uxs-nav-bar/title")
  spacer=(component "uxs-div" base=(concat b "__spacer"))
  menu=(
    component
    "uxs-menu"
    align="right"
    bordered=false
    horizontal=true
    class=(bem b "menu" m)
  )
  drawerToggle=(component "uxs-icon" "menu" color="white" onClick=(action "toggleDrawer"))
  drawer=(
    component
    "uxs-nav-bar/drawer"
    isShowingDrawer=isShowingDrawer
  )
  isShowingDrawer=this.isShowingDrawer
)}}
