{{yield
  (
    hash
    parent=this
    aside=(
      component
      "uxs-div"
      base=(concat b "__aside")
      m=m
    )
    aside-link=(
      component
      "uxs-link-to"
      base=(concat b "__aside")
      m=m
    )
    body=(
      component
      "uxs-div"
      base=(concat b "__body")
      m=m
    )
    body-link=(
      component
      "uxs-link-to"
      base=(concat b "__body")
      m=m
    )
    title=(
      component
      "uxs-subheading"
      base=(concat b "__title")
      m=m
    )
    content=(
      component
      "uxs-content"
      base=(concat b "__content")
      m=m
    )
    detail=(
      component
      "uxs-detail"
      base=(concat b "__detail")
      m=m
    )
    action=(
      component
      "uxs-icon"
      class=(bem b "action" m)
    )
    avatar=(
      component
      "uxs-avatar"
      class=(bem b "avatar" m) style=parentStyle
    )
    avatarIcon=(
      component
      "uxs-avatar-icon" class=(bem b "avatar" m) style=parentStyle
    )
    avatar-icon=(
      component
      "uxs-avatar-icon" class=(bem b "avatar" m) style=parentStyle
    )
    avatarText=(
      component
      "uxs-avatar-text"
      class=(bem b "avatar" m)
      style=parentStyle
    )

    avatar-text=(
      component
      "uxs-avatar-text"
      class=(bem b "avatar" m)
      style=parentStyle
    )
  )
}}
